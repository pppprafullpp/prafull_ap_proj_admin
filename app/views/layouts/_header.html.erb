<header id="header">
            <!--logo start-->
            <div class="brand">
                <a href="<%=root_url%>" class="logo"><span>Social</span>Booker</a>
            </div>
            <!--logo end-->
            <div class="toggle-navigation toggle-left">
                <button type="button" class="btn btn-default" id="toggle-left">
                    <i class="fa fa-bars"></i>
                </button>
            </div>
            <div class="user-nav">
                <ul>
                    <li class="dropdown messages">
                        <span class="badge badge-danager animated bounceIn" id="new-messages">38</span>
                        <button type="button" class="btn btn-default dropdown-toggle options" onclick="javascript:$('#messages-dropdown').toggle()" id="toggle-mail" data-toggle="dropdown">
                            <i class="fa fa-envelope"></i>
                        </button>
                        <ul class="dropdown-menu alert animated fadeInDown" id="messages-dropdown">
                            <li>
                                <h1>You have <strong>38</strong> new messages</h1>
                            </li>
                            <li>
                                <a href="#">
                                    <div class="profile-photo">
                                        <img src="/assets/member-img" alt="" class="img-circle">
                                    </div>
                                    <div class="message-info">
                                        <span class="sender">Ellen Baker</span>
                                        <span class="time">7 hours</span>
                                        <div class="message-content">Sem dapibus in, orci bibendum faucibus tellus, justo arcu...</div>
                                    </div>
                                </a>
                            </li>
                            <li><a href="#">Check all messages <i class="fa fa-angle-right"></i></a>
                            </li>
                        </ul>

                    </li>

                    <li class="dropdown messages">
                      <%notifications = get_notifications%>
                        <span id="notification_count" class="badge badge-danager animated bounceIn"><%=notifications.count%></span>
                        <button type="button" class="btn btn-default dropdown-toggle options" onclick="javascript:$('#notification-dropdown').toggle()" id="toggle-mail" data-toggle="dropdown">
                            <i class="fa fa-bell"></i>
                        </button>

                        <ul class="dropdown-menu alert animated fadeInDown" style="height: 189px;overflow: hidden;"id="notification-dropdown">
                            <%notifications.each do |n|%>
                              <li><a><div class="message-info"><%=n[1]%></div></a></li>
                            <%end%>
                        </ul>

                    </li>

                    <li class="profile-photo">
                        <img src="/assets/member-img.png" alt="" class="img-circle">
                    </li>
                    <li class="dropdown settings">
                        <a class="dropdown-toggle" data-toggle="dropdown" onclick="javascript:$('#admin-dropdown').toggle()" href="#">
                        <%=current_admin.name%> <i class="fa fa-angle-down"></i>
                    </a>
                        <ul class="dropdown-menu animated fadeInDown" id="admin-dropdown">
                            <li>
                                <a href="#"><i class="fa fa-user"></i> Profile</a>
                            </li>
                            <li>
                                <a href="#"><i class="fa fa-calendar"></i> Calendar</a>
                            </li>
                            <li>
                                <a href="#"><i class="fa fa-envelope"></i> Inbox <span class="badge badge-danager" id="user-inbox">5</span></a>
                            </li>
                            <li>
                                <a href="<%=destroy_admin_session_path%>" data-method="delete"><i class="fa fa-power-off"></i> Logout</a>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </header>

